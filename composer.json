{
    "name": "magp/yii2-adminlte3",
    "description": "AdminLTE backend theme asset bundle for Yii 2.0 Framework with Bootstrap 4",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","adminlte","admin","theme","backend","asset","css","less"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Manuel A. Gonzalez Prieto",
            "email": "manuel_alejandrogp@web.de"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-bootstrap4": "~1.0.0",
        "almasaeed2010/adminlte": "^3.0",
        "insolita/yii2-adminlte3-widgets": "~0.0.1"
    },
    "suggest": {
        "cebe/yii2-gravatar": "Display user icons",
        "rmrevin/yii2-fontawesome": "Font icons"
    },
    "autoload": {
        "psr-4": {
            "magp\\adminlte3\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
